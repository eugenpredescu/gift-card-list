    "babel": "^6.23.0",
    "co-body": "^6.0.0",
    "ramda": "^0.25.0",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-intl": "3",
    "react-test-renderer": "^17.0.2",
    "vtex": "^3.0.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "3",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.15.8",
    "@types/co-body": "^0.0.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.0.0",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/react": "^17.0.38",
    "@vtex/api": "6.45.6",
    "@vtex/test-tools": "^3.4.3",
    "@vtex/tsconfig": "^0.5.6",
    "babel-jest": "^27.4.6",
    "jest": "^27.0.3",
    "jest-css-modules": "^2.1.0",
    "ts-jest": "^27.0.3",
    "typescript": "3.9.7",
    "vtex.gift-card-list": "http://vtex.vtexassets.com/_v/public/typings/v1/vtex.gift-card-list@0.8.0/public/@types/vtex.gift-card-list",
    "vtex.list-graphql": "http://vtex.vtexassets.com/_v/public/typings/v1/vtex.list-graphql@1.10.0/public/@types/vtex.list-graphql",
    "vtex.render-runtime": "http://vtex.vtexassets.com/_v/public/typings/v1/vtex.render-runtime@8.132.3/public/@types/vtex.render-runtime",
    "vtex.styleguide": "http://vtex.vtexassets.com/_v/public/typings/v1/vtex.styleguide@9.145.2/public/@types/vtex.styleguide"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/+(spec|test).+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "lint": "tsc --noEmit --pretty",
    "test": "vtex-test-tools test"
  },
  "version": "0.9.0"
}
